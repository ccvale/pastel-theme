	"tokenColors": [
		{
			"name": "Comments",
			"scope": "comment",
			"settings": {
				"foreground": "#DF8598",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Constants",
			"scope": "constant",
			"settings": {
				"foreground": "#DF8598"
			}
		},
		{
			"name": "Escape characters",
			"scope": "constant.character.escape",
			"settings": {
				"foreground": "#AFCBFF"
			}
		},
		{
			"name": "Named entities",
			"scope": "entity.name",
			"settings": {
				"foreground": "#DF8598"
			}
		},
		{
			"name": "Function names",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#DF8598",
			}
		},
		{
			"name": "Tags",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#AA93DD",
			}
		},
		{
			"name": "Static types",
			"scope": [
				"entity.name.type",
				"storage.type.cs"
			],
			"settings": {
				"foreground": "#DF8598"
			}
		},
		{
			"name": "Tag attributes",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#afcbff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Inherited classes",
			"scope": "entity.other.inherited-class",
			"settings": {
				"foreground": "#DF8598"
			}
		},
		{
			"name": "CSS ID selectors",
			"scope": "entity.other.attribute-name.id",
			"settings": {
				"foreground": "#DF8598"
			}
		},
		{
			"name": "Function Operator Member C++",
			"scope": "entity.name.function.operator.member.cpp",
			"settings": {
				"foreground": "#D5D8DA",
			}
		},
		{
			"name": "Pseudo CSS",
